def calculator(a, b, operation):
    """
    A simple calculator function that performs basic operations.

    :param a: First number (float or int).
    :param b: Second number (float or int).
    :param operation: A string indicating the operation ('add', 'subtract', 'multiply', 'divide').
    :return: Result of the operation or a string indicating error.
    """
    try:
        if operation == 'add':
            return a + b
        elif operation == 'subtract':
            return a - b
        elif operation == 'multiply':
            return a * b
        elif operation == 'divide':
            if b != 0:
                return a / b
            else:
                return "Error: Division by zero"
        else:
            return "Error: Unsupported operation"
    except Exception as e:
        return f"Error: {str(e)}"

# Example usage
print(calculator(10, 5, 'add'))        # Expected output: 15
print(calculator(10, 5, 'subtract'))   # Expected output: 5
print(calculator(10, 5, 'multiply'))   # Expected output: 50
print(calculator(10, 5, 'divide'))     # Expected output: 2.0
```

